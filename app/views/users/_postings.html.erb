<% if @postings.nil? %>
  <% @group = Group.find(posting.group_id) if @group.nil? %>
  <% 
   if ( current_user.member?( @group.id ) )
     @postings = @group.postings.paginate(:page => params[:page], :per_page => 50 ).order('created_at DESC') #.limit(80)
   end 
   end 
 %>
<%= render 'shared/page_container', :page_container => @postings %>
