<% if session[:return_to] =~ /\/home?/ %>
$("#user_schools").html("<%= escape_javascript(render 'home/user_schools', :user => current_user ) %>");
	<% if current_user.schools.count <= max_num_schools %>
	$("#title_print_schools").html("<%= escape_javascript( render 'home/title_print_schools' ) %>");
	<% end %>
<% end %>

<% if flash.count > 0 || !notice.nil? %>
$(".info_note").html("<%= escape_javascript(render 'home/info') %>");
<% end %>
$(".loading").html("");