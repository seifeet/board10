<% if session[:return_to] =~ /\/home?/ %>
$('.generic_container .note').remove();
$('#user_postings .generic_container').prepend("<%= escape_javascript(render 'home/add_postings', :posting => @posting, :last_posting => @last_posting)%>");
$('#last_posting').val("<%= escape_javascript( @posting ? @posting.created_at.to_s : Time.now.to_s ) %>");
$('#auto_posting').val("<%= escape_javascript( @posting ? @posting.created_at.to_s : Time.now.to_s ) %>");
<% elsif session[:return_to] =~ /\/users\// %>
$('#user_postings').html("<%= escape_javascript(render 'users/postings', :posting => @posting)%>");
<% elsif session[:return_to] =~ /\/boards\// %>
$('#board_postings').html("<%= escape_javascript(render 'boards/postings', :posting => @posting)%>");
<% end %>
$(".loading").html("");
