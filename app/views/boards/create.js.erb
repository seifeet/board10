<% if stored_location_or_home =~ /\/home?/ %>
<% if flash.any? %>
$(".info_note").html("<%= escape_javascript(render 'home/info') %>");
<% else %>
$("#title_top_action").html("");
<% end %>
$("#navigation").html("<%= escape_javascript(render 'home/navigation' ) %>");
$("#title_print_boards").html("<%= escape_javascript(render 'home/title_print_boards' ) %>");
<% elsif stored_location_or_home =~ /\/users\// %>
$("#user_title_nav").html("<%= escape_javascript(render 'users/navigation') %>");
<% end %>

$(".loading").html("");
