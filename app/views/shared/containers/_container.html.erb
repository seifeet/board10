<% return nil if container.nil? || container.empty? || container.count == 0 %>

<div class="generic_container">
<% container.each do |element| %>
<%= render 'shared/elements/element', :element => element, :read_only => !current_user?(@user) %>
<% end %>
</div> <!-- cls-cntnr -->
<br class="clearBoth" />
